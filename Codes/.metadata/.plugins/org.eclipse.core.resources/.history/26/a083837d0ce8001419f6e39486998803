package model.server;

import vn.edu.cit.model.Server;

public class HelloMain {

	@SuppressWarnings("deprecation")
	public static void main(String[] args) throws InterruptedException {
		// Yeu cau ServerUsername phai dung
		Server sv1 = new Server("192.168.0.105", 22, "ubuntu", "mayb", "mayb");
		Server sv2 = new Server("192.168.0.101", 22, "ubuntu1", "ubuntu1",
				"ubuntu1");

		Server sv3 = new Server("192.168.0.106", 22, "servercontrol",
				"servercontrol", "servercontrol");
		// Chay Thread
		sv1.start();
		sv2.start();
		sv3.start();
		// Goi ham tat Thread
		// sv3.stop();

	}
}

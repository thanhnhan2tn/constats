package vn.edu.cit.model;

import java.util.Date;

import org.springframework.data.mongodb.core.mapping.Document;
/**
 * @author ThanhNhan
 *
 */
@Document(collection = "serverstatus")
public class ServerStatus {
	private Date time;
	private String cpu;
	private String ram;
	private String disk;
	private boolean up;

	
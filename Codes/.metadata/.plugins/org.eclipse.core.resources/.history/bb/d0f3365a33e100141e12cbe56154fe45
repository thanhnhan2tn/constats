package model.ftp;

import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;
import javax.xml.bind.annotation.XmlType;

@XmlRootElement(name = "ftp")
@XmlType(propOrder = { "listen", "anonymous_enable", "local_enable",
		"write_enable", "anon_upload_enable", "anon_mkdir_write_enable",
		"connect_from_port_20", "deny_email_enable", "chroot_local_user",
		"chroot_list_enable", "chroot_list_file" })
public class Ftp {
	private Boolean listen;
	private Boolean anonymous_enable;
	private Boolean local_enable;
	private Boolean write_enable;
	private Boolean anon_upload_enable;
	private Boolean anon_mkdir_write_enable;
	private Boolean connect_from_port_20;
	private Boolean deny_email_enable;
	private Boolean chroot_local_user;
	private Boolean chroot_list_enable;
	private String chroot_list_file;

	public Ftp() {
		// TODO Auto-generated constructor stub
	}

	public Ftp(Boolean listen, Boolean anonymous_enable, Boolean local_enable,
			Boolean write_enable, Boolean anon_upload_enable,
			Boolean anon_mkdir_write_enable, Boolean connect_from_port_20,
			Boolean deny_email_enable, Boolean chroot_local_user,
			Boolean chroot_list_enable, String chroot_list_file) {
		super();
		this.listen = listen;
		this.anonymous_enable = anonymous_enable;
		this.local_enable = local_enable;
		this.write_enable = write_enable;
		this.anon_upload_enable = anon_upload_enable;
		this.anon_mkdir_write_enable = anon_mkdir_write_enable;
		this.connect_from_port_20 = connect_from_port_20;
		this.deny_email_enable = deny_email_enable;
		this.chroot_local_user = chroot_local_user;
		this.chroot_list_enable = chroot_list_enable;
		this.chroot_list_file = chroot_list_file;
	}

	public Boolean getListen() {
		return listen;
	}

	public void setListen(Boolean listen) {
		this.listen = listen;
	}

	public Boolean getAnonymous_enable() {
		return anonymous_enable;
	}

	public void setAnonymous_enable(Boolean anonymous_enable) {
		this.anonymous_enable = anonymous_enable;
	}

	public Boolean getLocal_enable() {
		return local_enable;
	}

	public void setLocal_enable(Boolean local_enable) {
		this.local_enable = local_enable;
	}

	public Boolean getWrite_enable() {
		return write_enable;
	}

	public void setWrite_enable(Boolean write_enable) {
		this.write_enable = write_enable;
	}

	public Boolean getAnon_upload_enable() {
		return anon_upload_enable;
	}

	public void setAnon_upload_enable(Boolean anon_upload_enable) {
		this.anon_upload_enable = anon_upload_enable;
	}

	public Boolean getAnon_mkdir_write_enable() {
		return anon_mkdir_write_enable;
	}

	public void setAnon_mkdir_write_enable(Boolean anon_mkdir_write_enable) {
		this.anon_mkdir_write_enable = anon_mkdir_write_enable;
	}

	public Boolean getConnect_from_port_20() {
		return connect_from_port_20;
	}

	public void setConnect_from_port_20(Boolean connect_from_port_20) {
		this.connect_from_port_20 = connect_from_port_20;
	}

	public Boolean getDeny_email_enable() {
		return deny_email_enable;
	}

	public void setDeny_email_enable(Boolean deny_email_enable) {
		this.deny_email_enable = deny_email_enable;
	}

	public Boolean getChroot_local_user() {
		return chroot_local_user;
	}

	public void setChroot_local_user(Boolean chroot_local_user) {
		this.chroot_local_user = chroot_local_user;
	}

	public Boolean getChroot_list_enable() {
		return chroot_list_enable;
	}

	public void setChroot_list_enable(Boolean chroot_list_enable) {
		this.chroot_list_enable = chroot_list_enable;
	}

	public String getChroot_list_file() {
		return chroot_list_file;
	}

	public void setChroot_list_file(String chroot_list_file) {
		this.chroot_list_file = chroot_list_file;
	}

}

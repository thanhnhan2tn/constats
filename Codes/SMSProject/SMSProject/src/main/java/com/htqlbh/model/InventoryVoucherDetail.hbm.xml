<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.htqlbh.model.InventoryVoucherDetail" table="inventoryvoucherdetail" >
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <many-to-one name="stock" class="com.htqlbh.model.Stock" fetch="select">
            <column name="STOCKID" length="20" />
        </many-to-one>
        <many-to-one name="inventory" class="com.htqlbh.model.Inventory" fetch="select">
            <column name="INVTID" length="20" />
        </many-to-one>
        <many-to-one name="inventoryVoucher" class="com.htqlbh.model.InventoryVoucher" fetch="select">
            <column name="IVOUCHERNO" length="20" />
        </many-to-one>
        <property name="qty" type="java.lang.Integer">
            <column name="QTY" />
        </property>
    </class>
</hibernate-mapping>
